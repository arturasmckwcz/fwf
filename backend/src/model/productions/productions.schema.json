{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Production",
  "required": ["prescription_id", "source_id", "clinic_id", "code"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id in database"
    },
    "prescription_id": {
      "$id": "#/properties/prescription_id",
      "type": "integer",
      "title": "The FK prescription_id to prescription table"
    },
    "source_id": {
      "$id": "#/properties/source_id",
      "type": "integer",
      "title": "The FK source_id to source table"
    },
    "clinic_id": {
      "$id": "#/properties/clinic_id",
      "type": "integer",
      "title": "The FK clinic_id to clinic table"
    },
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The code of patient file"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The status of patient file"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The created_at timestamp"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The updated_at schema timestamp"
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The deleted_at timestamp"
    }
  },
  "additionalProperties": false
}
