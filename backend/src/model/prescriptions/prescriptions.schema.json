{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Lysate",
  "required": [
    "doctor_id",
    "patient_id",
    "product_id",
    "lysate_id",
    "code",
    "blood_source"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id in database"
    },
    "doctor_id": {
      "$id": "#/properties/doctor_id",
      "type": "integer",
      "title": "The FK doctor_id to doctor table"
    },
    "patient_id": {
      "$id": "#/properties/patient_id",
      "type": "integer",
      "title": "The FK patient_id to patient table"
    },
    "product_id": {
      "$id": "#/properties/product_id",
      "type": "integer",
      "title": "The FK product_id to product table"
    },
    "lysate_id": {
      "$id": "#/properties/lysate_id",
      "type": "integer",
      "title": "The FK lysate_id to lysate table"
    },
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "integer",
      "title": "The FK user_id to user table"
    },
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The code of patient file"
    },
    "blood_source": {
      "$id": "#/properties/blood_source",
      "type": "string",
      "title": "The status of patient file"
    },
    "issue_date": {
      "$id": "#/properties/issue_date",
      "type": "string",
      "title": "The issue date of the prescription"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The created_at timestamp"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The updated_at schema timestamp"
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The deleted_at timestamp"
    }
  },
  "additionalProperties": false
}
