{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Person",
    "required": [
        "first",
        "last",
        "gender",
        "age"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The id in database"
        },
        "first": {
            "$id": "#/properties/first",
            "type": "string",
            "title": "The given name"
        },
        "last": {
            "$id": "#/properties/last",
            "type": "string",
            "title": "The surname"
        },
        "gender": {
            "$id": "#/properties/gender",
            "type": "string",
            "title": "The gender"
        },
        "age": {
            "$id": "#/properties/age",
            "type": "integer",
            "title": "The age"
        },
        "address": {
            "$id": "#/properties/address",
            "type": "string",
            "title": "The address"
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email"
        },
        "phone": {
            "$id": "#/properties/phone",
            "type": "string",
            "title": "The phone numbet"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The time record created_at"
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The time record updated_at"
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The time record deleted_at"
        }
    },
    "additionalProperties": false
}